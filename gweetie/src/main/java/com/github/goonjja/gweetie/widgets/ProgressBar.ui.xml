<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<g:HTMLPanel>
		<g:HTMLPanel ui:field='progressBar' styleName="progress progress-striped active"
			width="100%">
			<div ui:field='progressBarValueDiv' class="bar" width="0"></div>
		</g:HTMLPanel>
		<i>
			<g:InlineLabel ui:field='progressText' />
		</i>
	</g:HTMLPanel>
</ui:UiBinder> 